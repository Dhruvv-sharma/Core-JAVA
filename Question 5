//Write a Function to find a number is even or odd?
import java.util.Scanner;
public class Ifelse {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a Number.");
		int n =sc.nextInt();
		if(n%2==0)
			System.out.println("Your Number is Even Number : "+(n));
		else
			System.out.println("Your Number is Odd Number : "+(n));	
	}
}
